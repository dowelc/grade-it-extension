{
	"manifest_version": 2,
	"name": "Change Grade-It Colors",
	"version": "0.1",
	"browser_action": {
		"default_icon": "icons/gradeit_icon.png",
		"default_popup": "popup.html"
	},
	"icons": {
		"48": "icons/gradeit_icon.png"
	},
	"content_scripts": [
	  	{
		    "matches": [
		      	"https://gradeit.cs.washington.edu/uwcse/admin/multipane_view.php?*"
		    ],
		    "js": ["content.js", "jquery-3.1.1.js"]
	  	}
	],
	"permissions": [
		"storage"
	]
}